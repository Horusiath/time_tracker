#project
  %h1= "#{@current_project.name} - project page"

  %section
    %fieldset.well
      %legend Owners
      %table.table.table-striped.table-bordered
        %thead
          %tr
            %th Email
        %tbody
          - @current_project.our_owner_emails.each do |o|
            %tr
              %td
                = o

